{
    "compilerOptions": {
        "target": "ES2023",
        "lib": [
            "ES2023"
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "removeComments": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "skipLibCheck": true,
        "noFallthroughCasesInSwitch": false
    }
}